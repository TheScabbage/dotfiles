[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = master
[core]
	editor = nvim
[column]
	ui = auto
[branch]
	sort = -committerdate
[maintenance]
	repo = /home/scabbage/projects/dust
